{
  "name": "@redux-multipurpose/angular-cli",
  "version": "0.0.1",
  "description": "Helper cli for an easy @redux-multipurpose packages integration",
  "main": "./lib/index.js",
  "bin": {
    "multux": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "sudo npm i -g && multux",
    "clean": "rm -rf ./node_modules ./package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KitCarson88/redux-multipurpose-angular-cli.git"
  },
  "keywords": [
    "@redux-multipurpose",
    "angular",
    "cli"
  ],
  "author": "Simone Colazzo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KitCarson88/redux-multipurpose-angular-cli/issues"
  },
  "homepage": "https://github.com/KitCarson88/redux-multipurpose-angular-cli#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^5.1.0",
    "figlet": "^1.4.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}
